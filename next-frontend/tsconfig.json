{
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "dom",
      "esnext"
    ],
    "jsx": "preserve",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "types": [
      "next",
      "next-auth"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "next-auth.d.ts" // Explicitly include the type definition
, "components/RegisterPageModal.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}